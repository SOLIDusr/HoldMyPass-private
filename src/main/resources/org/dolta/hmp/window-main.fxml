<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="600" maxWidth="900" prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #2E3348;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.dolta.hmp.MainController">
    <AnchorPane prefHeight="40.0" prefWidth="900.0" style="-fx-background-color: #FAFAFA;">
        <Label layoutX="379.0" layoutY="4.0" prefHeight="33.0" prefWidth="143.0" text="HoldMyPass!" textFill="#2f344a">
            <font>
                <Font name="Comic Sans MS Italic" size="23.0" />
            </font>
        </Label>
    </AnchorPane>
    <AnchorPane layoutY="40.0" prefHeight="562.0" prefWidth="293.0" style="-fx-background-color: #778899;">
        <Label layoutX="7.0" layoutY="14.0" prefHeight="33.0" prefWidth="81.0" text="Application" textFill="#2f344a">
            <font>
                <Font name="Comic Sans MS Italic" size="13.0" />
            </font>
        </Label>
        <Button fx:id="addAppButton" contentDisplay="CENTER" layoutX="4.0" layoutY="512.0" mnemonicParsing="false" text="Add Application" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS">
            <font>
                <Font name="Comic Sans MS Italic" size="12.0" />
            </font>
        </Button>
        <Button fx:id="addAccountButton" layoutX="5.0" layoutY="306.0" mnemonicParsing="false" text="Add Account" textOverrun="CENTER_WORD_ELLIPSIS" />
        <Button fx:id="showMeButton" layoutX="5.0" layoutY="351.0" mnemonicParsing="false" text="Show me!" />
        <ChoiceBox fx:id="accountSelector" layoutX="7.0" layoutY="42.0" prefHeight="27.0" prefWidth="187.0" />
        <TextField fx:id="loginTextField" layoutX="6.0" layoutY="221.0" prefHeight="26.0" prefWidth="283.0" promptText="login">
            <font>
                <Font name="Comic Sans MS" size="12.0" />
            </font>
        </TextField>
        <TextField fx:id="passTextField" layoutX="5.0" layoutY="262.0" prefHeight="26.0" prefWidth="283.0" promptText="password">
            <font>
                <Font name="Comic Sans MS" size="12.0" />
            </font>
        </TextField>
        <TextField fx:id="appNameTextField" layoutX="4.0" layoutY="478.0" promptText="Application">
            <font>
                <Font name="Comic Sans MS" size="12.0" />
            </font>
        </TextField>
    </AnchorPane>
    <TableView fx:id="logTable" layoutX="292.0" layoutY="40.0" prefHeight="562.0" prefWidth="608.0" style="-fx-background-color: #2E3348;">
        <columns>
            <TableColumn prefWidth="75.0" text="Login" />
            <TableColumn prefWidth="75.0" text="Password" />
        </columns>
    </TableView>
</AnchorPane>
